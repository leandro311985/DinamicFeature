<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_app"
    app:startDestination="@id/navigation_login">


    <include-dynamic
        android:id="@id/navigation_login"
        android:name="com.example.dinamicfeature.login"
        app:graphPackage="com.example.dinamicfeature.login"
        app:graphResName="navigation_login"
        app:moduleName="login" />

    <include-dynamic
        android:id="@id/navigation_home"
        android:name="com.example.dinamicfeature.home"
        app:graphPackage="com.example.dinamicfeature.home"
        app:graphResName="navigation_home"
        app:moduleName="home" />

  <include-dynamic
        android:id="@id/navigation_profile"
        android:name="com.example.dinamicfeature.profile"
        app:graphPackage="com.example.dinamicfeature.profile"
        app:graphResName="navigation_profile"
        app:moduleName="@string/title_profile" />

    <include-dynamic
        android:id="@id/navigation_signature"
        android:name="com.example.dinamicfeature.mysignature"
        app:graphPackage="com.example.dinamicfeature.mysignature"
        app:graphResName="navigation_signature"
        app:moduleName="@string/title_mysignature" />

    <include-dynamic
        android:id="@id/navigation_search_people"
        android:name="com.example.dinamicfeature.searchepeople"
        app:graphPackage="com.example.dinamicfeature.searchpeople"
        app:graphResName="navigation_search_people"
        app:moduleName="@string/title_searchpeople" />

</navigation>