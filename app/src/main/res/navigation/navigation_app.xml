<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_app"
    app:startDestination="@id/navigation_login">

    <include-dynamic
        android:id="@id/navigation_login"
        android:name="com.example.dinamicfeature.login"
        app:graphPackage="${applicationId}.login"
        app:graphResName="navigation_login"
        app:moduleName="@string/title_login" />

    <include-dynamic
        android:id="@id/navigation_home"
        android:name="com.example.dinamicfeature.home"
        app:graphPackage="${applicationId}.home"
        app:graphResName="navigation_home"
        app:moduleName="@string/title_home" />


</navigation>