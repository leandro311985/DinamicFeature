<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_success"
        android:layout_width="250dp"
        android:layout_height="@dimen/DEFAULT_250DP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/appCompatImageView"
            android:layout_width="260dp"
            android:layout_height="250dp"
            android:scaleType="centerCrop"
            android:elevation="@dimen/DEFAULT_50DP"
            android:src="@drawable/ic_logo_success_one"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textOk"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="COMEÃ‡AR A JORNADA"
        android:textColor="@color/DEFAULT_WHITE"
        android:textSize="@dimen/DEFAULT_20SP"
        android:layout_marginEnd="@dimen/DEFAULT_20DP"
        android:layout_marginStart="@dimen/DEFAULT_20DP"
        android:gravity="center"
        android:layout_marginBottom="@dimen/DEFAULT_20DP"
        android:background="@color/DEFAULT_AZUL_APP_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>