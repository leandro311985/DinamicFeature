<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/idIVText"
        android:layout_width="0dp"
        android:fontFamily="@font/swiss721"
        android:layout_height="200dp"
        android:background="@drawable/path673"
        android:justificationMode="inter_word"
        android:textSize="@dimen/DEFAULT_20SP"
        android:layout_marginEnd="@dimen/DEFAULT_40DP"
        android:layout_marginStart="@dimen/DEFAULT_40DP"
        android:layout_marginTop="@dimen/DEFAULT_150DP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>